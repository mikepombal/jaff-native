{
  "name": "jaffNative",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-open": "open ios/*.xcodeproj",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "link": "node node_modules/react-native/local-cli/cli.js link",
    "lint": "eslint src test *.js",
    "test": "mocha --recursive --compilers js:babel-core/register -r test/setup",
    "cover": "babel-node node_modules/.bin/isparta cover _mocha -- --recursive -r test/setup",
    "remotedev": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "postinstall": "npm run link && npm run remotedev && rndebugger-open"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.3.0",
    "react": "15.1.0",
    "react-native": "^0.28.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-react-native": "^1.4.0",
    "chai": "^3.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.1",
    "eslint-plugin-react": "^5.1.1",
    "immutable-devtools": "0.0.7",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "node-fetch": "^1.3.3",
    "proxyquire": "^1.7.4",
    "react-addons-test-utils": "15.1.0",
    "react-dom": "15.1.0",
    "react-native-debugger-open": "^0.2.1",
    "react-native-mock": "^0.2.3",
    "remote-redux-devtools": "^0.3.1",
    "remote-redux-devtools-on-debugger": "^0.4.4",
    "sinon": "^1.17.2"
  }
}
